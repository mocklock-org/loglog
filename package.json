{
  "name": "loglog-core",
  "version": "1.2.2",
  "description": "A scalable, framework-agnostic logging system with built-in support for various frameworks",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "start": "ts-node examples/basic.ts",
    "prepare": "npm run build",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "docs": "typedoc --out docs src"
  },
  "keywords": [
    "logger",
    "logging",
    "express-middleware",
    "nextjs",
    "typescript",
    "observability",
    "monitoring"
  ],
  "author": "Paul Mothapo <paul@paulmothapo.personal@gmail.com> (https://paulmothapo.co.za)",
  "license": "MIT",
  "dependencies": {
    "@opentelemetry/api": "^1.7.0",
    "@opentelemetry/auto-instrumentations-node": "^0.40.2",
    "@opentelemetry/sdk-node": "^0.45.1",
    "chalk": "^4.1.2",
    "express": ">=4.17.1",
    "logform": "^2.7.0",
    "moment": "^2.29.4",
    "pino": "^8.17.2",
    "rotating-file-stream": "^3.2.6",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.5",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "next": "^15.3.3",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typedoc": "^0.25.4",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "express": ">=4.17.1",
    "next": ">=12.0.0"
  },
  "peerDependenciesMeta": {
    "express": {
      "optional": true
    },
    "next": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mocklock-org/loglog.git"
  },
  "bugs": {
    "url": "https://github.com/mocklock-org/loglog/issues"
  },
  "homepage": "https://github.com/mocklock-org/loglog#readme",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
